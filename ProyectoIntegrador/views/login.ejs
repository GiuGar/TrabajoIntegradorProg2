<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<!-- Head -->
<%- include('./partials/head') %>	
<!-- /Head -->

<body>
	<!-- Header -->
	<%- include('./partials/headerDeslogueado') %>	
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Ingresar</h2>
			</div>

			<form action="/login" method="GET">
				<label for="email">Email:</label>
				<input type="email" id="email" name="email" required>
			  
				<label for="password">Contraseña:</label>
				<input type="password" id="password" name="password" required>
			  
				<button type="submit">Iniciar sesión</button>
			  </form>

			<div class="register">	
				<div class="input_field">
				  <input type="text" placeholder="Email" class="input" value="">
				</div>
				<div class="input_field">
				  <input type="text" placeholder="Usuario" class="input" value="">
				</div>
				<div class="input_field">
				  <input type="password" placeholder="Contraseña" class="input" value="">
				</div>
				<div class="input_field"></div>
					<input type="number" placeholder="DNI" max="8" value="">
				</div>
				<div class="input_field"></div>
					<input type="date" placeholder="Fecha de Nacimiento" class="input" value="">
				</div>
				<button type=submit><a href="login">Registrarse</a></button>
			  </div>


	<!-- footer -->
	<%- include('./partials/footer') %>	
	<!-- /footer -->
</body>

</html>